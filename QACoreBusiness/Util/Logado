using System;
using System.Collections.Generic;
using System.Text;
using OpenQA.Selenium;
using OpenQA.Selenium.Chrome;
using Xunit;

namespace QACoreBusiness.Util
{
  
    public class Logado
    {
        AbrirNavegadorUtil abrirNavegador = new AbrirNavegadorUtil();
        
        public void fazerLogin(){
             abrirNavegador.IniciarNavegador();
             abrirNavegador.TelaDeLogin();
             abrirNavegador.InsereDados();
             abrirNavegador.CliqueEntrarSistema();
             abrirNavegador.NavegadorAberto();
        }

}
